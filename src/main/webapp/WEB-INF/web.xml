<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <display-name>Welcome to Tomcat</display-name>
    <description>
        Welcome to Tomcat
    </description>
    <servlet>
        <servlet-name>locationCheck</servlet-name>
        <servlet-class>servlet.WifiInformationServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>locationCheck</servlet-name>
        <url-pattern>/locationCheck</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>history</servlet-name>
        <servlet-class>history.HistoryServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>history</servlet-name>
        <url-pattern>/history</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>creatDB</servlet-name>
        <servlet-class>servlet.CreateDBServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>creatDB</servlet-name>
        <url-pattern>/creatDB</url-pattern>
    </servlet-mapping>

</web-app>
